{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Security": {
    "ChallengeTtlSeconds": 60
  },
  "RateLimiting": {
    "PerIpTokens": 30,
    "PerIpRefillPerSecond": 1,
    "PerUserTokens": 20,
    "PerUserRefillPerSecond": 1
  },
  "AllowedOrigins": [ "http://localhost:3000", "https://localhost:3000" ]
}
